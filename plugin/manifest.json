{
  "manifest_version": 2,
  "name": "Go Assistant",
  "description": "Go Assistant",
  "version": "1.0",
  "background": {
    "scripts": ["main.js"]
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["listener.js"]
    }
  ],
  "permissions": ["nativeMessaging", "contextMenus"]
}
